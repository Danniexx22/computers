{% extends 'base.html.twig' %}

{% block title %}Jefatura index{% endblock %}

{% block contenido %}
    <h1>Jefatura index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Descripcion</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for jefatura in jefaturas %}
            <tr>
                <td>{{ jefatura.id }}</td>
                <td>{{ jefatura.descripcion }}</td>
                <td>
                    <a href="{{ path('jefatura_show', {'id': jefatura.id}) }}">show</a>
                    <a href="{{ path('jefatura_edit', {'id': jefatura.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('jefatura_new') }}">Create new</a>
{% endblock %}